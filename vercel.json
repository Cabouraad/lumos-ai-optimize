{
  "rewrites": [
    {
      "source": "/resources",
      "destination": "/resources/index.html",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(Googlebot|Bingbot|Slurp|DuckDuckBot|Baiduspider|YandexBot|facebookexternalhit|Twitterbot|LinkedInBot|Applebot|PetalBot|SemrushBot|AhrefsBot|MJ12bot|Screaming Frog|Bytespider|GPTBot|ChatGPT-User|anthropic-ai|ClaudeBot|PerplexityBot|Google-Extended)"
        }
      ]
    },
    {
      "source": "/pricing",
      "destination": "/pricing/index.html",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(Googlebot|Bingbot|Slurp|DuckDuckBot|Baiduspider|YandexBot|facebookexternalhit|Twitterbot|LinkedInBot|Applebot|PetalBot|SemrushBot|AhrefsBot|MJ12bot|Screaming Frog|Bytespider|GPTBot|ChatGPT-User|anthropic-ai|ClaudeBot|PerplexityBot|Google-Extended)"
        }
      ]
    },
    {
      "source": "/features",
      "destination": "/features/index.html",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(Googlebot|Bingbot|Slurp|DuckDuckBot|Baiduspider|YandexBot|facebookexternalhit|Twitterbot|LinkedInBot|Applebot|PetalBot|SemrushBot|AhrefsBot|MJ12bot|Screaming Frog|Bytespider|GPTBot|ChatGPT-User|anthropic-ai|ClaudeBot|PerplexityBot|Google-Extended)"
        }
      ]
    },
    {
      "source": "/((?!api|_next|static|favicon.ico|robots.txt|sitemap.xml|.*\\..*).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}