# Supabase Configuration
VITE_SUPABASE_PROJECT_ID=your_project_id_here
VITE_SUPABASE_URL=https://your_project_id.supabase.co
VITE_SUPABASE_ANON_KEY=your_publishable_anon_key_here

# Next.js compatibility (optional, but recommended for future-proofing)
NEXT_PUBLIC_SUPABASE_URL=https://your_project_id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_publishable_anon_key_here

# Debug overlay to help diagnose "Failed to fetch"
VITE_DEBUG_HEALTH=false
NEXT_PUBLIC_DEBUG_HEALTH=false

OPENAI_API_KEY=
PERPLEXITY_API_KEY=

# Google AI Overviews Configuration (optional)
SERPAPI_KEY=your_serpapi_key_here
ENABLE_GOOGLE_AIO=true

# Model overrides (optional) 
OPENAI_MODEL=gpt-4o-mini
PERPLEXITY_MODEL=sonar
# cost can be edited later; store and surface token counts regardless
COST_PER_1K_INPUT_OPENAI=0.00
COST_PER_1K_OUTPUT_OPENAI=0.00
COST_PER_1K_INPUT_PERPLEXITY=0.00
COST_PER_1K_OUTPUT_PERPLEXITY=0.00